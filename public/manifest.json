{
  "name": "Tv Wizard",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Gives the best tv shows",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
  "background": {
    "scripts": ["background.js"]
   },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
   "permissions": ["contextMenus", "tabs"],
   "web_accessible_resources" : ["*.html"]
}
